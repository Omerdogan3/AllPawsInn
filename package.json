{
  "name": "electron-react-bootstrap-template",
  "version": "1.0.0",
  "description": "template for using electron with react and bootstrap",
  "main": "main.js",
  "repository": "https://github.com/Shemnei/electron-react-bootstrap-template",
  "scripts": {
    "start": "electron -r babel-register .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . allpawsinn --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"allpawsinn\"",
    "package-linux": "electron-packager . allpawsinn --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Jonas Grawe",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.6.2"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "bootstrap": "^3.3.7",
    "electron-packager": "^12.0.2",
    "jquery": "^3.1.1",
    "moment": "^2.21.0",
    "moment-range": "^3.1.1",
    "mssql": "^4.1.0",
    "npm": "^6.0.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-data-grid": "^3.0.11",
    "react-datepicker": "^1.4.1",
    "react-day-picker": "^7.1.6",
    "react-dom": "^15.4.2",
    "react-grid-layout": "^0.16.6",
    "react-infinite-calendar": "^2.3.1",
    "react-input-calendar": "^0.5.2",
    "react-selectable": "^2.0.1",
    "remote": "^0.2.6"
  }
}
